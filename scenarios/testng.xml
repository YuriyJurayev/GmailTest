<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
   <test name="Test on chrome">
       <parameter name="browser" value="CHROME"/>
       <parameter name="login" value="first_login"/>
       <parameter name="password" value="first_password"/>
        <classes>
            <class name="kz.epam.atm.gmailtestPF.tests.GmailTest"/>
            <class name="kz.epam.atm.gmailtestPF.tests.UsingJSExecutorAndActionsGmailTest"/>
        </classes>
    </test>
    <test name="Test on firefox">
        <parameter name="browser" value="CHROME"/>
        <parameter name="login" value="second_login"/>
        <parameter name="password" value="second_password"/>
        <classes>
            <class name="kz.epam.atm.gmailtestPF.tests.GmailTest"/>
            <class name="kz.epam.atm.gmailtestPF.tests.UsingJSExecutorAndActionsGmailTest"/>
        </classes>
    </test>
</suite>
